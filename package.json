{
  "name": "pagy-templates-1",
  "version": "0.0.1",
  "description": "cool template",
  "author": "ichiki1023",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "dev": "node server.js",
    "build:app": "next build app",
    "build": "run-s build:app copy-static",
    "start": "node server.js",
    "copy-static": "cpx \"app/static/**/*.*\" \"static\"",
    "format": "run-s format:*",
    "format:js": "prettier-standard ./app/**/*.js",
    "format:others": "prettier --write ./app/**/*.{css,json}",
    "lint": "standard .app/**/*.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "google-map-react": "^1.1.2",
    "ip-range-check": "0.0.2",
    "is_js": "^0.9.0",
    "jss": "^9.8.7",
    "mobile-detect": "^1.4.3",
    "moment": "^2.24.0",
    "next": "^8.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-dropdown": "^1.6.3",
    "react-responsive": "^6.0.1",
    "react-scroll": "^1.7.10",
    "react-slick": "^0.23.2",
    "react-svg": "^7.2.3",
    "request-ip": "^2.1.3",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.1.3",
    "url-loader": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "cpx": "^1.5.0",
    "dotenv-webpack": "^1.6.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prettier-standard": "^8.0.1",
    "standard": "^12.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "linters": {
      "app/**/*.js": [
        "prettier-standard ./app/**/*.js",
        "standard ./app/**/*.js",
        "git add"
      ],
      "app/**/*.css": [
        "prettier --write ./app/**/*.css",
        "git add"
      ]
    }
  },
  "standard": {
    "parser": "babel-eslint"
  }
}